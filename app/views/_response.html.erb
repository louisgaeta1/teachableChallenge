<h2>FOUND GEM</h2>
<a href="<%=@gem['project_uri']%>"><%=@gem['name']%></a><%= image_tag 'star-gray.png', class: 'star'%>
<h2>INFO</h2>
<p><%= @gem['info']%></p>
<h2>DEPENDENCIES</h2>
<%if @dependencies.length > 0 %>
  <ul>
    <% @dependencies.each do |dep| %>
      <li><a href="<%=dep['project_uri']%>"><%=dep['name']%></a> <%= image_tag 'star-gray.png', class:'star'%> </li>
    <% end %>
  </ul>
<%else%>
  <p>None</p>
<% end %>
